            {% if not error %}
            <div class="accordion-item">
                <h2 class="accordion-header" id="headingThree">
                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                        data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                        Odkazy pro tento dotaz
                    </button>
                </h2>
                <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree"
                    data-bs-parent="#upravaDotazu">
                    <div class="container mt-2">
                        <ul class="list-group">
                            <!-- First item -->
                            <li class="list-group-item">
                                <h5 class="mb-1">Odkaz na tento dashboard:</h5>
                                <a href="{{urls["this"]}}" target="_blank">{{urls["this"]}}</a>
                            </li>
                            <!-- Second item -->
                            <li class="list-group-item">
                                <h5 class="mb-1">Odkaz na stažení dat pomocí API:</h5>
                                <a href="{{urls["api"]}}" target="_blank">{{urls["api"]}}</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            {% endif %}
        </div>
        <!-- Uprava dotazu konec-->

        {% if not error %}

        <div class="container mt-5">
            <h3>Data pro rok {{data["parameters"]["end_year"]}}</h3>
            <div class="row mt-3">

                <!-- soucet -->
                <div class="col-md-4">
                    <div class="containter">
                        <h4>Součet:</h4>
                        <h4>{{data["totalCount"]["current"]}}</h4>
                    </div>
                    {% if data["totalCount"]["last"] %}
                    <div class="containter mt-3">
                        <h5>Minulý rok:</h5>
                        <h6>{{data["totalCount"]["last"]}} ({{data["totalCount"]["change"]}})</h6>
                    </div>
                    {% endif %}
                    
                </div>
                <!-- soucet -->

                <!-- Vekova pyramida -->
                <div class="col-md-4">
                    <h4>Věková pyramida</h4>
                    <div class="container mt-3">
                        <canvas id="ageChart" height="350"></canvas>
                    </div>
                    <script>
                        const totalPopulationPyramidctx = document.getElementById('ageChart').getContext('2d');
                        const totalPopulationPyramid = new Chart(totalPopulationPyramidctx, {
                            type: 'bar',
                            data: {
                                labels: {{data["ageChart"]["labels"]}}, // Age groups
                                datasets: [{
                                    label: ' počet s daným věkem',
                                    data: {{data["ageChart"]["values"]}}, // Sum of both genders for each age group
                                    backgroundColor: 'rgba(100, 149, 237, 0.6)', // Color for the bars
                                    borderColor: 'rgba(100, 149, 237, 1)',
                                    borderWidth: 1
                                }]
                            },
                            options: {
                                indexAxis: 'y', // Makes the bar chart horizontal
                                responsive: true,
                                scales: {
                                    x: {
                                        beginAtZero: true, // Start the x-axis from 0
                                    }
                                },
                                plugins: {
                                    legend: {
                                        display: false, // This will hide the legend
                                    }
                                }
                            }
                        });
                    </script>
                </div>
                <!-- Vekova pyramida -->

                <!-- graf pohlavi  -->
                <div class="col-md-4">
                    <h4>Složení pohlaví</h4>
                    <div class="container mt-3">
                        <canvas id="pieChart"></canvas>
                    </div>
                    <script>
                        const genderPieChartCtx = document.getElementById('pieChart').getContext('2d');
                        const genderPieChart = new Chart(genderPieChartCtx, {
                            type: 'pie', // Define chart type
                            data: {
                                labels: {{ data["pieData"]["labels"] }}, // These are your labels for the data points
                                datasets: [{
                                    data: {{ data["pieData"]["values"] }}, 
                                    backgroundColor: [
                                        'rgba(54, 162, 235, 0.6)', // Color for Male, e.g., light blue
                                        'rgba(255, 99, 132, 0.6)'  // Color for Female, e.g., light red
                                    ],
                                    borderColor: [
                                        'rgba(54, 162, 235, 1)', // Border color for Male
                                        'rgba(255, 99, 132, 1)'  // Border color for Female
                                    ],
                                    borderWidth: 1 // Set border width
                                }]
                            },
                            options: {
                                plugins: {
                                    legend: {
                                        position: 'top', // Position of the legend
                                    }
                                }
                            }
                        });
                    </script>
                </div>
                <!-- graf pohlavi -->
            </div>
        </div>

        {% if data["chartData"]["display"] or data["subregionYearTable"]["display"] or data["nationalityYearTable"]["display"]%}
        <div class="container mt-5">
            <h3>Data pro zvolené roky</h3>
        

            <!-- Casovy graf  -->
            {% if data["chartData"]["display"] %}
            <div class="containter mt-3">
                <h4>Zobrazení počtu cizinců v čase</h4>
                <div class="container mt-3">
                    <canvas id="timeChart" height="80"></canvas>
                </div>
                <script>
                const ctx = document.getElementById('timeChart');
                new Chart(ctx, {
                    type: 'bar',
                    data: {
                    labels: {{ data["chartData"]["labels"] }}, // roky
                    datasets: [{
                        label: 'počet cizinců',
                        data: {{data["chartData"]["values"]}}, // cizinci
                        borderWidth: 1
                    }]  
                    },
                    options: {
                        scales: {
                            y: {
                            beginAtZero: true
                            }
                        },
                        responsive: true,
                        plugins: {
                            legend: {
                                display: false, // This will hide the legend
                            }
                        }
                    }
                });
                </script>
            </div>
            {% endif %}
            <!-- Casovy graf konec -->

            <!-- Tabulka podregion / rok  -->
            {% if data["subregionYearTable"]["display"] %}
            <div class="container mt-3 table-responsive">
                <h4>Tabulka podoblast / rok</h4>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col"></th>
                            {% for header in data["subregionYearTable"]["headers"] %}
                            <th scope="col">{{header}}</th>
                            {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                        {% for x in data["subregionYearTable"]["first_col"] %}
                            <tr>
                                <th scope="row">{{ data["subregionYearTable"]["first_col"][loop.index0]}}</th>
                                {% set rowloop = loop %}
                                {% for value in data["subregionYearTable"]["data"][loop.index0] %}
                                    {% if rowloop.last %}
                                        <th>{{ value }}</th>
                                    {% else %}
                                        <td>{{ value }}</td>
                                    {% endif %}
                                {% endfor %}
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
            {% endif %}
            <!-- Tabulka podregion / rok  -->

            <!-- Tabulka narodnost / rok  -->
            {% if data["nationalityYearTable"]["display"] %}
            <div class="container mt-3 table-responsive">
                <h4>Tabulka národnost / rok</h4>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col"></th>
                            {% for header in data["nationalityYearTable"]["headers"] %}
                            <th scope="col">{{header}}</th>
                            {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                        {% for x in data["nationalityYearTable"]["first_col"] %}
                            <tr>
                                <th scope="row">{{ data["nationalityYearTable"]["first_col"][loop.index0]}}</th>
                                {% for value in data["nationalityYearTable"]["data"][loop.index0] %}
                                <td>{{ value }}</td>
                                {% endfor %}
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
            {% endif %}
            <!-- Tabulka narodnost / rok  -->
        </div>
        {% endif %}
    {% endif %}


